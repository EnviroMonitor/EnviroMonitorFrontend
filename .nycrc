{
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
       "app/**/*.js",
       "app/**/*.jsx"
    ],
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true
  },
  "reporter": [
    "text",
    "text-summary",
    "html"
  ]
}